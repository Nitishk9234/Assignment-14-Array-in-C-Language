#include<stdio.h>
int main()
{
    int num[10],i,se=0,so=0;
    printf("enter 10 number :");
    for(i=0;i<=9;i++)
    {
        scanf("%d",&num[i]);
    if(num[i]%2==0)
        se=se+num[i];
    else
        so=so+num[i];
    }
    printf("sum of all even number is %d \n",se);
    printf("sum of all odd number is %d",so);
    return 0;
}
